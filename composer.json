{
  "name": "alecrabbit/php-counters",
  "type": "library",
  "description": "Awesome package description",
  "license": "MIT",
  "require": {
    "php": ">=7.2",
    "alecrabbit/php-helpers": "^0.5 || ^0.6 || ^0.8",
    "alecrabbit/php-traits": "^0.3",
    "alecrabbit/php-reports": "^0.2",
    "illuminate/container": "^5.8"
  },
  "require-dev": {
    "nunomaduro/collision": "^3.0",
    "phpunit/phpunit": "^8.0",
    "symfony/var-dumper": "^4.2"
  },
  "autoload": {
    "psr-4": {
      "AlecRabbit\\Counters\\": "src\\Counters",
      "AlecRabbit\\Formatters\\": "src\\Formatters",
      "AlecRabbit\\Reports\\": "src\\Reports"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AlecRabbit\\Tests\\Counters\\": "tests",
      "AlecRabbit\\Tests\\": "tests"
    }
  },
  "minimum-stability": "beta",
  "prefer-stable": true
}
